<script language="JavaScript" type="text/javascript">
    $(function () {
        //面包屑
        var drop = $('.drop'),
            navDrop = $('.nav-drop'),
            navigation = $('.sub-nav'), offset_top, h;

        if(drop.length){
            drop.mouseenter(function(ev) {
                var target = $(ev.currentTarget);
                clearTimeout(target.data('hideTimer'));
                target.addClass('open');
                target.children('bd').fadeIn(200);
            });
            drop.mouseleave(function(ev) {
                var target = $(ev.currentTarget);
                target.data("hideTimer", setTimeout(function() {
                    target.removeClass('open');
                    target.children('bd').fadeOut(200);
                }, 100));
            });
        }
        //城市概况
        if(navDrop.length){
            navDrop.mouseenter(function(ev) {
                var target = $(ev.currentTarget);
                clearTimeout(target.data('hideTimer'));
                target.addClass('drop-open');
                target.children('drop-bd').fadeIn(200);
            });
            navDrop.mouseleave(function(ev) {
                var target = $(ev.currentTarget);
                target.data("hideTimer", setTimeout(function() {
                    target.removeClass('drop-open');
                    target.children('drop-bd').fadeOut(200);
                }, 100));
            });
        }
        if(navigation.length){
            offset_top = navigation.offset().top,
            h = navigation.outerHeight(true);
            !$('#fill_area')[0] && navigation.after('<div id="fill_area" style="height: '+h+'px;display:none;"></div>');
            function setFixed(){
                var scrolltop = $(document).scrollTop(),
                    fillarea = $('#fill_area');
                if(scrolltop  > offset_top){
                    fillarea.show();
                    navigation.addClass('nav-fixed');
                }else{
                    fillarea.hide();
                    navigation.removeClass('nav-fixed');
                }
            }
            $(window.document).scroll(function(){
                setFixed();
            });
            setFixed();
        }
    });
</script>
